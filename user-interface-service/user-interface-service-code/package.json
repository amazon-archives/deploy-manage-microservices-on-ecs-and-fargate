{
  "name": "user-management",
  "version": "1.0.0",
  "description": "user management example app",
  "main": "app.js",
  "scripts": {
    "start": "npm run concat-js; npm run uglifyjs; npm run less; npm run concat-css; npm run minify-css; NODE_ENV=production PORT=3000 node app.js",
    "development": "npm run less; npm run concat-css; NODE_ENV=development PORT=5000 NODE_ENV=development nodemon app.js -e js,html",
    "concat-js": "rm -f public/js/combined.js; rm -f public/js/combined.min.js; concat-cli -f public/js/* -o public/js/combined.js",
    "uglifyjs": "touch public/js/combined.min.js; uglifyjs public/js/combined.js -o public/js/combined.min.js",
    "less": "lessc public/less/main.less public/css/main.css",
    "concat-css": "concat-cli -f public/css/font-awesome.min.css public/css/css-helpers.min.css public/css/main.css -o public/css/combined.css",
    "minify-css": "touch public/css/combined.min.css; cleancss -o public/css/combined.min.css public/css/combined.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amazon-cognito-identity-js": "^1.28.0",
    "async": "^2.6.0",
    "aws-sdk": "^2.205.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "clean-css": "^4.2.3",
    "colors": "^1.2.1",
    "compression": "^1.7.1",
    "concat-cli": "^4.0.0",
    "connect-dynamodb": "^2.0.1",
    "connect-redis": "^3.3.3",
    "connect-redis-multiple": "^0.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csv-parse": "^2.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fast-csv": "^2.4.1",
    "hogan-express": "^0.5.2",
    "install": "^0.4.4",
    "less": "^2.7.3",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "multer": "^1.3.0",
    "npm": "^3.10.10",
    "redis": "^2.8.0",
    "uglify-js": "^2.8.29"
  }
}
